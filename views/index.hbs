<main id="content" class="container">
    
    <div class="row">
        {{#each data}}
        <div class="col-md-4">
            <div class="card" style="width: 18rem;">
                <img src="" class="card-img-top" alt="...">
                <div class="card-body">
                    <h1 class="card-title">{{nombre}}</h1>
                    <h2>{{precio}}</h2>
                    <p class="card-text">{{descripcion}}</p>
                    <a href="#" class="btn btn-primary">Comprar</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</main>
